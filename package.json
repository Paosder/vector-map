    "test": "yarn test:coverage && yarn test:lint",
    "test:coverage": "jest --no-cache",
    "test:lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0",
    "prepare": "husky install",
    "docgen": "typedoc src/map.ts",
    "release": "standard-version"
  }
